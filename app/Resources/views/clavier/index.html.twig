    <div class="container">
        <div class="row">
            <div class="col-xs-12"><p id="resume"></p></div>
            <div class="col-xs-12"><p>{% if equipe.adverse is defined and equipe.adverse is not same as ("")%}{{ equipe.adverse }}{% else %}adversaire{% endif %} : <span id="butadv">0</span> - <span id="butclub">0</span>  {{ entraineur.nomClub }} </p></div>


        </div>

        <div class="keyboard">
            <div class="row">
                <div value="0123" class="col-xs-4 adverse_pad highlight_pads counter-click joueur" id="adversaire"
                     data-color="red" >
                    <p>{% if equipe.adverse is defined and equipe.adverse is not same as ("")%}{{ equipe.adverse }}{% else %}Adverse{% endif %}<br></p>
